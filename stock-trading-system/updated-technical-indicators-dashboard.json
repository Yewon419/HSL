{
  "dashboard": {
    "id": null,
    "uid": "tech-indicators-002",
    "title": "Technical Indicators Dashboard - Updated",
    "tags": ["stock", "technical-analysis", "indicators"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 2,
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "name": "ticker",
          "type": "query",
          "current": {
            "text": "005930 - 삼성전자",
            "value": "005930"
          },
          "datasource": {
            "type": "postgres",
            "uid": "${DS_POSTGRESQL}"
          },
          "definition": "SELECT ticker AS __value, ticker || ' - ' || company_name AS __text FROM stocks WHERE is_active = true AND ticker IN (SELECT DISTINCT ticker FROM stock_prices WHERE date >= CURRENT_DATE - INTERVAL '30 days') ORDER BY CASE WHEN ticker = '005930' THEN 1 WHEN ticker = '000660' THEN 2 WHEN ticker = '035420' THEN 3 ELSE 4 END, company_name",
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "label": "종목선택",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0
        }
      ]
    },
    "time": {
      "from": "now-3M",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 0},
        "type": "timeseries",
        "title": "가격 차트 with 이동평균",
        "datasource": {
          "type": "postgres",
          "uid": "${DS_POSTGRESQL}"
        },
        "targets": [
          {
            "rawSql": "SELECT date as time, close_price as \"종가\" FROM stock_prices WHERE ticker = '$ticker' AND date >= $__timeFrom() AND date <= $__timeTo() ORDER BY date",
            "format": "time_series",
            "refId": "A"
          },
          {
            "rawSql": "SELECT date as time, (value::json->>'sma_5')::float as \"SMA 5\" FROM indicator_values WHERE ticker = '$ticker' AND indicator_id = 1 AND date >= $__timeFrom() AND date <= $__timeTo() ORDER BY date",
            "format": "time_series",
            "refId": "B"
          },
          {
            "rawSql": "SELECT date as time, (value::json->>'sma_20')::float as \"SMA 20\" FROM indicator_values WHERE ticker = '$ticker' AND indicator_id = 1 AND date >= $__timeFrom() AND date <= $__timeTo() ORDER BY date",
            "format": "time_series",
            "refId": "C"
          },
          {
            "rawSql": "SELECT date as time, (value::json->>'sma_50')::float as \"SMA 50\" FROM indicator_values WHERE ticker = '$ticker' AND indicator_id = 1 AND date >= $__timeFrom() AND date <= $__timeTo() ORDER BY date",
            "format": "time_series",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "가격",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 0,
              "gradientMode": "none",
              "hideFrom": {"tooltip": false, "viz": false, "legend": false},
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": true,
              "stacking": {"group": "A", "mode": "none"}
            },
            "unit": "short"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "종가"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}},
                {"id": "custom.lineWidth", "value": 3}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "SMA 5"},
              "properties": [{"id": "color", "value": {"fixedColor": "yellow", "mode": "fixed"}}]
            },
            {
              "matcher": {"id": "byName", "options": "SMA 20"},
              "properties": [{"id": "color", "value": {"fixedColor": "blue", "mode": "fixed"}}]
            },
            {
              "matcher": {"id": "byName", "options": "SMA 50"},
              "properties": [{"id": "color", "value": {"fixedColor": "orange", "mode": "fixed"}}]
            }
          ]
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single",
            "sort": "none"
          }
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10},
        "type": "timeseries",
        "title": "RSI (Relative Strength Index)",
        "datasource": {
          "type": "postgres",
          "uid": "${DS_POSTGRESQL}"
        },
        "targets": [
          {
            "rawSql": "SELECT date as time, (value::json->>'rsi')::float as \"RSI\" FROM indicator_values WHERE ticker = '$ticker' AND indicator_id = 1 AND date >= $__timeFrom() AND date <= $__timeTo() ORDER BY date",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "color": {"mode": "thresholds"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": true,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 30},
                {"color": "red", "value": 70}
              ]
            }
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10},
        "type": "timeseries",
        "title": "MACD",
        "datasource": {
          "type": "postgres",
          "uid": "${DS_POSTGRESQL}"
        },
        "targets": [
          {
            "rawSql": "SELECT date as time, (value::json->>'macd')::float as \"MACD\", (value::json->>'macd_signal')::float as \"Signal\", (value::json->>'macd_hist')::float as \"Histogram\" FROM indicator_values WHERE ticker = '$ticker' AND indicator_id = 1 AND date >= $__timeFrom() AND date <= $__timeTo() ORDER BY date",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": true,
              "showPoints": "never"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Histogram"},
              "properties": [
                {"id": "custom.drawStyle", "value": "bars"},
                {"id": "custom.fillOpacity", "value": 50}
              ]
            }
          ]
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 18},
        "type": "timeseries",
        "title": "볼린저 밴드 (Bollinger Bands)",
        "datasource": {
          "type": "postgres",
          "uid": "${DS_POSTGRESQL}"
        },
        "targets": [
          {
            "rawSql": "SELECT date as time, close_price as \"종가\" FROM stock_prices WHERE ticker = '$ticker' AND date >= $__timeFrom() AND date <= $__timeTo() ORDER BY date",
            "format": "time_series",
            "refId": "A"
          },
          {
            "rawSql": "SELECT date as time, (value::json->>'bb_upper')::float as \"상한선\", (value::json->>'bb_middle')::float as \"중간선\", (value::json->>'bb_lower')::float as \"하한선\" FROM indicator_values WHERE ticker = '$ticker' AND indicator_id = 1 AND date >= $__timeFrom() AND date <= $__timeTo() ORDER BY date",
            "format": "time_series",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": true,
              "showPoints": "never"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "종가"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}},
                {"id": "custom.lineWidth", "value": 2}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "상한선"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}},
                {"id": "custom.lineStyle", "value": {"dash": [10, 10], "fill": "dash"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "하한선"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}},
                {"id": "custom.lineStyle", "value": {"dash": [10, 10], "fill": "dash"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "중간선"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "blue", "mode": "fixed"}},
                {"id": "custom.lineStyle", "value": {"dash": [5, 5], "fill": "dash"}}
              ]
            }
          ]
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      }
    ]
  }
}