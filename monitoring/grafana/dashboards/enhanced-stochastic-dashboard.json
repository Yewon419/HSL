{
  "dashboard": {
    "id": null,
    "uid": "enhanced-stoch-001",
    "title": "Enhanced Stochastic Dashboard",
    "tags": ["stock", "stochastic", "technical-analysis"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "name": "ticker",
          "type": "query",
          "current": {
            "text": "005930 - 삼성전자",
            "value": "005930"
          },
          "datasource": {
            "type": "postgres",
            "uid": "${DS_POSTGRESQL}"
          },
          "definition": "SELECT ticker AS __value, ticker || ' - ' || company_name AS __text FROM stocks WHERE is_active = true AND ticker IN (SELECT DISTINCT ticker FROM stock_prices WHERE date >= CURRENT_DATE - INTERVAL '30 days') ORDER BY CASE WHEN ticker = '005930' THEN 1 WHEN ticker = '000660' THEN 2 WHEN ticker = '035420' THEN 3 ELSE 4 END, company_name",
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "label": "종목선택",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0
        }
      ]
    },
    "time": {
      "from": "now-3M",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "type": "timeseries",
        "title": "Stock Price",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  date AS time,\n  close_price AS \"Close Price\"\nFROM stock_prices\nWHERE ticker = '$ticker'\n  AND date >= NOW() - INTERVAL '3 months'\nORDER BY date",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "Price",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 0,
              "gradientMode": "none",
              "hideFrom": {"tooltip": false, "viz": false, "legend": false},
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": true,
              "stacking": {"group": "A", "mode": "none"}
            },
            "unit": "currencyKRW"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Close Price"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}},
                {"id": "custom.lineWidth", "value": 3}
              ]
            }
          ]
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8},
        "type": "timeseries",
        "title": "Stochastic Oscillator (%K 빠른선 & %D 느린선)",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  date AS time,\n  (value->>'stoch_k')::numeric AS \"%K (Fast Line)\",\n  (value->>'stoch_d')::numeric AS \"%D (Slow Line)\"\nFROM indicator_values iv\nJOIN indicator_definitions id ON iv.indicator_id = id.id\nWHERE iv.ticker = '$ticker'\n  AND id.code = 'STOCH'\n  AND date >= NOW() - INTERVAL '3 months'\n  AND (value->>'stoch_k') IS NOT NULL\n  AND (value->>'stoch_d') IS NOT NULL\nORDER BY date",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "hideFrom": {"tooltip": false, "viz": false, "legend": false},
              "spanNulls": true,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "transparent", "value": null},
                {"color": "transparent", "value": 20},
                {"color": "transparent", "value": 80}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "%K (Fast Line)"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "blue", "mode": "fixed"}},
                {"id": "custom.lineWidth", "value": 3},
                {"id": "displayName", "value": "%K (빠른선)"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "%D (Slow Line)"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}},
                {"id": "custom.lineWidth", "value": 3},
                {"id": "displayName", "value": "%D (느린선)"}
              ]
            }
          ]
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true,
            "values": ["last", "min", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          }
        },
        "transformations": [],
        "transparent": false
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 18},
        "type": "stat",
        "title": "Current Stochastic Values",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  (value->>'stoch_k')::numeric AS \"Stoch %K\",\n  (value->>'stoch_d')::numeric AS \"Stoch %D\",\n  CASE \n    WHEN (value->>'stoch_k')::numeric > (value->>'stoch_d')::numeric THEN '%K > %D (상승 신호)'\n    WHEN (value->>'stoch_k')::numeric < (value->>'stoch_d')::numeric THEN '%K < %D (하락 신호)'\n    ELSE '중립'\n  END AS \"Signal\"\nFROM indicator_values iv\nJOIN indicator_definitions id ON iv.indicator_id = id.id\nWHERE iv.ticker = '$ticker'\n  AND id.code = 'STOCH'\n  AND (value->>'stoch_k') IS NOT NULL\n  AND (value->>'stoch_d') IS NOT NULL\nORDER BY date DESC\nLIMIT 1",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "custom": {
              "align": "center",
              "displayMode": "basic"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 20},
                {"color": "yellow", "value": 80}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 18},
        "type": "table",
        "title": "Recent Stochastic History",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  date AS \"Date\",\n  ROUND((value->>'stoch_k')::numeric, 2) AS \"Stoch %K\",\n  ROUND((value->>'stoch_d')::numeric, 2) AS \"Stoch %D\",\n  CASE \n    WHEN (value->>'stoch_k')::numeric > (value->>'stoch_d')::numeric THEN 'K > D'\n    WHEN (value->>'stoch_k')::numeric < (value->>'stoch_d')::numeric THEN 'K < D'\n    ELSE 'Neutral'\n  END AS \"Signal\"\nFROM indicator_values iv\nJOIN indicator_definitions id ON iv.indicator_id = id.id\nWHERE iv.ticker = '$ticker'\n  AND id.code = 'STOCH'\n  AND (value->>'stoch_k') IS NOT NULL\n  AND (value->>'stoch_d') IS NOT NULL\nORDER BY date DESC\nLIMIT 10",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "center",
              "displayMode": "basic"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Stoch %K"},
              "properties": [
                {"id": "custom.displayMode", "value": "color-background"},
                {"id": "color", "value": {"mode": "continuous-BlPu"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Stoch %D"},
              "properties": [
                {"id": "custom.displayMode", "value": "color-background"},
                {"id": "color", "value": {"mode": "continuous-RdYlGn"}}
              ]
            }
          ]
        }
      }
    ]
  },
  "overwrite": true
}