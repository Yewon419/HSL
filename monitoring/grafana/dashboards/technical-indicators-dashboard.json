{
  "dashboard": {
    "id": null,
    "uid": "tech-indicators-001",
    "title": "Technical Indicators Dashboard",
    "tags": ["stock", "technical-analysis", "indicators"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "name": "ticker",
          "type": "query",
          "current": {
            "text": "005930 - 삼성전자",
            "value": "005930"
          },
          "datasource": {
            "type": "postgres",
            "uid": "${DS_POSTGRESQL}"
          },
          "definition": "SELECT ticker AS __value, ticker || ' - ' || company_name AS __text FROM stocks WHERE is_active = true AND ticker IN (SELECT DISTINCT ticker FROM stock_prices WHERE date >= CURRENT_DATE - INTERVAL '30 days') ORDER BY CASE WHEN ticker = '005930' THEN 1 WHEN ticker = '000660' THEN 2 WHEN ticker = '035420' THEN 3 ELSE 4 END, company_name",
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "label": "종목선택",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0
        }
      ]
    },
    "time": {
      "from": "now-3M",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "type": "timeseries",
        "title": "Price & Moving Averages",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  date AS time,\n  close_price AS \"Price\",\n  (SELECT (value->>'sma_20')::numeric FROM indicator_values iv JOIN indicator_definitions id ON iv.indicator_id = id.id WHERE iv.ticker = '$ticker' AND id.code = 'SMA' AND iv.date = sp.date) AS \"SMA 20\",\n  (SELECT (value->>'sma_50')::numeric FROM indicator_values iv JOIN indicator_definitions id ON iv.indicator_id = id.id WHERE iv.ticker = '$ticker' AND id.code = 'SMA' AND iv.date = sp.date) AS \"SMA 50\",\n  (SELECT (value->>'sma_200')::numeric FROM indicator_values iv JOIN indicator_definitions id ON iv.indicator_id = id.id WHERE iv.ticker = '$ticker' AND id.code = 'SMA' AND iv.date = sp.date) AS \"SMA 200\"\nFROM stock_prices sp\nWHERE ticker = '$ticker'\n  AND date >= NOW() - INTERVAL '$timeRange'\nORDER BY date",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "Price",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 0,
              "gradientMode": "none",
              "hideFrom": {"tooltip": false, "viz": false, "legend": false},
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": true,
              "stacking": {"group": "A", "mode": "none"}
            },
            "unit": "currencyKRW"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Price"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}},
                {"id": "custom.lineWidth", "value": 3}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "SMA 20"},
              "properties": [{"id": "color", "value": {"fixedColor": "blue", "mode": "fixed"}}]
            },
            {
              "matcher": {"id": "byName", "options": "SMA 50"},
              "properties": [{"id": "color", "value": {"fixedColor": "orange", "mode": "fixed"}}]
            },
            {
              "matcher": {"id": "byName", "options": "SMA 200"},
              "properties": [{"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}}]
            }
          ]
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 8},
        "type": "timeseries",
        "title": "MACD",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  date AS time,\n  (value->>'macd')::numeric AS \"MACD\",\n  (value->>'macd_signal')::numeric AS \"Signal\",\n  (value->>'macd_histogram')::numeric AS \"Histogram\"\nFROM indicator_values iv\nJOIN indicator_definitions id ON iv.indicator_id = id.id\nWHERE iv.ticker = '$ticker'\n  AND id.code = 'MACD'\n  AND date >= NOW() - INTERVAL '$timeRange'\nORDER BY date",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": true,
              "showPoints": "never"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Histogram"},
              "properties": [
                {"id": "custom.drawStyle", "value": "bars"},
                {"id": "custom.fillOpacity", "value": 50}
              ]
            }
          ]
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 8},
        "type": "timeseries",
        "title": "RSI",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  date AS time,\n  (value->>'rsi')::numeric AS \"RSI\"\nFROM indicator_values iv\nJOIN indicator_definitions id ON iv.indicator_id = id.id\nWHERE iv.ticker = '$ticker'\n  AND id.code = 'RSI'\n  AND date >= NOW() - INTERVAL '$timeRange'\nORDER BY date",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": true,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 30},
                {"color": "green", "value": 40},
                {"color": "green", "value": 60},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 80}
              ]
            }
          }
        },
        "options": {
          "legend": {"calcs": ["last"], "displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "single"}
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "type": "timeseries",
        "title": "Stochastic Oscillator (%K 빠른선 & %D 느린선)",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  date AS time,\n  (value->>'stoch_k')::numeric AS \"%K (Fast Line)\",\n  (value->>'stoch_d')::numeric AS \"%D (Slow Line)\"\nFROM indicator_values iv\nJOIN indicator_definitions id ON iv.indicator_id = id.id\nWHERE iv.ticker = '$ticker'\n  AND id.code = 'STOCH'\n  AND date >= NOW() - INTERVAL '$timeRange'\n  AND (value->>'stoch_k') IS NOT NULL\n  AND (value->>'stoch_d') IS NOT NULL\nORDER BY date",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "hideFrom": {"tooltip": false, "viz": false, "legend": false},
              "spanNulls": true,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "transparent", "value": null}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "%K (Fast Line)"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "blue", "mode": "fixed"}},
                {"id": "custom.lineWidth", "value": 3},
                {"id": "displayName", "value": "%K (빠른선)"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "%D (Slow Line)"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}},
                {"id": "custom.lineWidth", "value": 3},
                {"id": "displayName", "value": "%D (느린선)"}
              ]
            }
          ]
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true,
            "values": ["current", "min", "max"]
          },
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 14},
        "type": "timeseries",
        "title": "Bollinger Bands",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  sp.date AS time,\n  sp.close_price AS \"Price\",\n  (iv.value->>'bb_upper')::numeric AS \"Upper Band\",\n  (iv.value->>'bb_middle')::numeric AS \"Middle Band\",\n  (iv.value->>'bb_lower')::numeric AS \"Lower Band\"\nFROM stock_prices sp\nLEFT JOIN indicator_values iv ON sp.ticker = iv.ticker AND sp.date = iv.date\nLEFT JOIN indicator_definitions id ON iv.indicator_id = id.id AND id.code = 'BB'\nWHERE sp.ticker = '$ticker'\n  AND sp.date >= NOW() - INTERVAL '$timeRange'\nORDER BY sp.date",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": true
            },
            "unit": "currencyKRW"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Price"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}},
                {"id": "custom.lineWidth", "value": 3}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Upper Band"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}},
                {"id": "custom.lineStyle", "value": {"fill": "dash", "dash": [10, 10]}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Lower Band"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "blue", "mode": "fixed"}},
                {"id": "custom.lineStyle", "value": {"fill": "dash", "dash": [10, 10]}}
              ]
            }
          ]
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 20},
        "type": "stat",
        "title": "Current Indicators",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  current_price,\n  ROUND(rsi, 2) as rsi,\n  ROUND(macd, 2) as macd,\n  ROUND(adx, 2) as adx,\n  ROUND(atr, 2) as atr\nFROM v_indicator_summary\nWHERE ticker = '$ticker'\nLIMIT 1",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {
            "values": true,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value_and_name",
          "graphMode": "none"
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 20},
        "type": "table",
        "title": "Trading Signals",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "SELECT \n  ticker AS \"Ticker\",\n  company_name AS \"Company\",\n  current_price AS \"Price\",\n  rsi_signal AS \"RSI Signal\",\n  macd_signal_type AS \"MACD Signal\",\n  stoch_signal AS \"Stoch Signal\",\n  trend_signal AS \"Trend\",\n  trend_strength AS \"Strength\"\nFROM v_trading_signals\nWHERE ticker = '$ticker'\nLIMIT 1",
            "format": "table",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 20},
        "type": "gauge",
        "title": "Market Sentiment",
        "datasource": "PostgreSQL",
        "targets": [
          {
            "rawSql": "WITH sentiment AS (\n  SELECT \n    CASE \n      WHEN rsi < 30 THEN 20\n      WHEN rsi < 40 THEN 35\n      WHEN rsi < 60 THEN 50\n      WHEN rsi < 70 THEN 65\n      WHEN rsi >= 70 THEN 80\n      ELSE 50\n    END as sentiment_score\n  FROM v_indicator_summary\n  WHERE ticker = '$ticker'\n)\nSELECT sentiment_score FROM sentiment",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 30},
                {"color": "green", "value": 40},
                {"color": "green", "value": 60},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "percent"
          }
        },
        "options": {
          "orientation": "auto",
          "showThresholdLabels": true,
          "showThresholdMarkers": true,
          "text": {
            "titleSize": 16,
            "valueSize": 32
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "ticker",
          "type": "query",
          "datasource": "PostgreSQL",
          "query": "SELECT ticker FROM stocks WHERE is_active = true ORDER BY ticker",
          "current": {
            "text": "005930",
            "value": "005930"
          },
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "options": [],
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "tagValuesQuery": "",
          "tagsQuery": "",
          "useTags": false
        },
        {
          "name": "timeRange",
          "type": "custom",
          "current": {
            "text": "3 months",
            "value": "3 months"
          },
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "options": [
            {"text": "1 week", "value": "1 week"},
            {"text": "1 month", "value": "1 month"},
            {"text": "3 months", "value": "3 months"},
            {"text": "6 months", "value": "6 months"},
            {"text": "1 year", "value": "1 year"}
          ],
          "query": "1 week,1 month,3 months,6 months,1 year",
          "skipUrlSync": false,
          "type": "custom"
        }
      ]
    },
    "time": {
      "from": "now-3M",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d", "90d", "180d", "1y"]
    }
  },
  "overwrite": true
}